<?xml version='1.0' encoding='utf-8'?>
<widget id="des.neoris.neoCover" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NEO Cover</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="Orientation" value="portrait" />
    <preference name="EnableViewportScale" value="true" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" src="resources/android/icon/neocover-mdpi-icon.png" />
        <icon density="hdpi" src="resources/android/icon/neocover-hdpi-icon.png" />
        <icon density="xhdpi" src="resources/android/icon/neocover-xhdpi-icon.png" />
        <icon density="xxhdpi" src="resources/android/icon/neocover-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="resources/android/icon/neocover-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="resources/android/neocover.png" />
        <splash density="land-mdpi" src="resources/android/neocover.png" />
        <splash density="land-hdpi" src="resources/android/neocover.png" />
        <splash density="land-xhdpi" src="resources/android/neocover.png" />
        <splash density="land-xxhdpi" src="resources/android/neocover.png" />
        <splash density="land-xxxhdpi" src="resources/android/neocover.png" />
        <splash density="port-ldpi" src="resources/android/neocover.png" />
        <splash density="port-mdpi" src="resources/android/neocover.png" />
        <splash density="port-hdpi" src="resources/android/neocover.png" />
        <splash density="port-xhdpi" src="resources/android/neocover.png" />
        <splash density="port-xxhdpi" src="resources/android/neocover.png" />
        <splash density="port-xxxhdpi" src="resources/android/neocover.png" />
    </platform>
    <platform name="ios">
        <config-file parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
            <string>Necesitamos la camara para realizar las fotos.</string>
        </config-file>
        <config-file parent="NSLocationWhenInUseUsageDescription" platform="ios" target="*-Info.plist">
            <string>Necesitamos la localizacion para realizar saber donde estas.</string>
        </config-file>
        <config-file parent="NSPhotoLibraryUsageDescription" platform="ios" target="*-Info.plist">
            <string>Necesitamos el acceso a la galeria para guardar las imagenes.</string>
        </config-file>
        <config-file parent="NSMicrophoneUsageDescription" platform="ios" target="*-Info.plist">
            <string>Necesitamos acceso al microfono para grabar el audo.</string>
        </config-file>
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/icon/neocover.png" width="57" />
        <icon height="114" src="resources/ios/icon/neocover@2x.png" width="114" />
        <icon height="40" src="resources/ios/icon/neocover-40.png" width="40" />
        <icon height="80" src="resources/ios/icon/neocover-40@2x.png" width="80" />
        <icon height="120" src="resources/ios/icon/neocover-40@3x.png" width="120" />
        <icon height="50" src="resources/ios/icon/neocover-50.png" width="50" />
        <icon height="100" src="resources/ios/icon/neocover-50@2x.png" width="100" />
        <icon height="60" src="resources/ios/icon/neocover-60.png" width="60" />
        <icon height="120" src="resources/ios/icon/neocover-60@2x.png" width="120" />
        <icon height="180" src="resources/ios/icon/neocover-60@3x.png" width="180" />
        <icon height="72" src="resources/ios/icon/neocover-72.png" width="72" />
        <icon height="144" src="resources/ios/icon/neocover-72@2x.png" width="144" />
        <icon height="76" src="resources/ios/icon/neocover-76.png" width="76" />
        <icon height="152" src="resources/ios/icon/neocover-76@2x.png" width="152" />
        <icon height="167" src="resources/ios/icon/neocover-83.5@2x.png" width="167" />
        <icon height="29" src="resources/ios/icon/neocover-small.png" width="29" />
        <icon height="58" src="resources/ios/icon/neocover-small@2x.png" width="58" />
        <icon height="87" src="resources/ios/icon/neocover-small@3x.png" width="87" />
        <icon height="1024" src="resources/ios/icon/neocover-1024.png" width="1024" />
        <splash height="1136" src="resources/ios/splash/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="resources/ios/splash/Default-667h.png" width="750" />
        <splash height="2208" src="resources/ios/splash/Default-736h.png" width="1242" />
        <splash height="1242" src="resources/ios/splash/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" src="resources/ios/splash/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="2048" src="resources/ios/splash/Default-Landscape@~ipadpro.png" width="2732" />
        <splash height="768" src="resources/ios/splash/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" src="resources/ios/splash/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="2732" src="resources/ios/splash/Default-Portrait@~ipadpro.png" width="2048" />
        <splash height="1024" src="resources/ios/splash/Default-Portrait~ipad.png" width="768" />
        <splash height="960" src="resources/ios/splash/Default@2x~iphone.png" width="640" />
        <splash height="480" src="resources/ios/splash/Default~iphone.png" width="320" />
        <splash height="2732" src="resources/ios/splash/Default@2x~universal~anyany.png" width="2732" />
    </platform>
    <engine name="browser" spec="5.0.3" />
    <engine name="android" spec="6.3.0" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-x-toast" spec="^2.7.0" />
    <plugin name="cordova-plugin-media-capture" spec="^3.0.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^1.2.1" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.1.2" />
    <plugin name="cordova-plugin-nativeaudio" spec="^3.0.9" />
    <plugin name="cordova-plugin-media" spec="^5.0.2" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="facephi-plugin-widget" spec="file:facephi-plugin-widget" />
    <plugin name="cordova-android-support-gradle-release" spec="^1.4.4">
        <variable name="ANDROID_SUPPORT_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-speechrecognition" spec="^1.2.0" />
    <plugin name="cordova-plugin-tts" spec="^0.2.3" />
</widget>
